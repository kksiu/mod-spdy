{
  "manifest_version": 2,

  "name": "Header Jacker",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  /*
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },*/

  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage",
    "alarms",
    "chrome://*/*",
    "webRequest",
    "webRequestBlocking"
  ]
}
